import java.util.Scanner;

public class 달팽이_2869 {
	public static void main(String[] args) {
		System.out.println("V, A, B");
		Scanner sc = new Scanner(System.in);
		int A= sc.nextInt();
		int B= sc.nextInt(); 
		int V= sc.nextInt();
		
		int day = (V - B) / (A - B);
		   
		System.out.println(day);
		
	}

}